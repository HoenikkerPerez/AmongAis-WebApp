node

.load totally-unreliable-game-client.js
.load game-client.js
client = new GameClient();

(async ()=>{
    try {
        console.debug("Connecting...");
        await client.connect();
        console.debug("Connected.");
        (async ()=>{
            try {
                console.debug("Creating a Game...");
                await client.createGame("telnet-web-test-1")
                console.debug("Game created.");
            } catch(err) {
                console.debug(err);
            }
        })();
    } catch(err) {
        console.debug(err);
    }
})();

process.exit();